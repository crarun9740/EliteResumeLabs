@charset "utf-8";

body { font-family: Arial, sans-serif; line-height: 1.6; color: rgb(51, 51,=
 51); margin: 0px; padding: 0px; background-color: rgb(244, 244, 244); }

.resume { max-width: 800px; margin: 20px auto; background-color: rgb(255, 2=
55, 255); padding: 40px; box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 10px; }

h1, h2, h3 { margin: 0px 0px 10px; }

h1 { font-size: 36px; text-align: center; color: rgb(44, 62, 80); }

h2 { font-size: 24px; color: rgb(44, 62, 80); border-bottom: 2px solid rgb(=
52, 152, 219); padding-bottom: 5px; margin-top: 30px; }

h3 { font-size: 18px; color: rgb(44, 62, 80); }

.header { text-align: center; margin-bottom: 30px; }

.title { font-size: 18px; color: rgb(127, 140, 141); margin: 10px 0px; }

.contact { font-size: 14px; }

.contact a { color: rgb(52, 152, 219); text-decoration: none; }

.contact a:hover { text-decoration: underline; }

.summary, .experience, .education, .achievements, .skills, .certification, =
.passions, .languages { margin-bottom: 30px; }

.job, .education-item { margin-bottom: 20px; }

.job-header, .edu-header { display: flex; justify-content: space-between; a=
lign-items: flex-start; margin-bottom: 5px; }

.job-info, .edu-info { text-align: right; color: rgb(127, 140, 141); font-s=
ize: 14px; }

.job-title { font-style: italic; color: rgb(127, 140, 141); margin: 0px 0px=
 10px; }

ul { padding-left: 20px; margin-top: 10px; }

li { margin-bottom: 5px; }

.achievements-grid, .passions-grid { display: grid; grid-template-columns: =
repeat(3, 1fr); gap: 20px; }

.achievement, .passion { background-color: rgb(249, 249, 249); padding: 15p=
x; border-radius: 5px; text-align: center; }

.achievement-icon, .passion-icon { font-size: 24px; margin-bottom: 10px; }

.achievement h3, .passion h3 { color: rgb(52, 152, 219); margin-top: 0px; }

.skills p { display: flex; flex-wrap: wrap; gap: 10px; }

.cert-item { margin-bottom: 15px; }

.language-grid { display: grid; grid-template-columns: repeat(2, 1fr); gap:=
 15px; }

.language { display: flex; flex-direction: column; align-items: center; }

.lang-name { font-weight: bold; }

.proficiency { font-size: 14px; color: rgb(127, 140, 141); }

.proficiency-dots { color: rgb(52, 152, 219); letter-spacing: 2px; }

@media (max-width: 768px) {
  .resume { padding: 20px; }
  .job-header, .edu-header { flex-direction: column; }
  .job-info, .edu-info { text-align: left; margin-top: 5px; }
  .achievements-grid, .passions-grid { grid-template-columns: 1fr; }
  .language-grid { grid-template-columns: 1fr; }
}
